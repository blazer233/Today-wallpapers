{
  "name": "my-app",
  "version": "0.1.0",
  "private": true,
  "main": "public/electron.js",
  "homepage": ".",
  "executablePath": "C:\\Users\\darylsong\\AppData\\Local\\Google\\Chrome\\Application\\chrome.exe",
  "devDependencies": {
    "electron": "^11.2.2",
    "electron-builder": "^22.9.1",
    "nodemon": "^2.0.7",
    "wait-on": "^5.2.1"
  },
  "dependencies": {
    "@testing-library/jest-dom": "^5.11.4",
    "@testing-library/react": "^11.2.5",
    "@testing-library/user-event": "^12.6.3",
    "antd": "^4.12.0",
    "babel-plugin-import": "^1.13.3",
    "concurrently": "^5.3.0",
    "cross-env": "^7.0.3",
    "customize-cra": "^1.0.0",
    "dayjs": "^1.10.4",
    "electron-dl": "^3.0.2",
    "electron-log": "^4.3.1",
    "electron-store": "^7.0.1",
    "puppeteer": "^7.0.1",
    "react": "^17.0.1",
    "react-app-rewired": "^2.1.8",
    "react-dom": "^17.0.1",
    "react-load-script": "^0.0.6",
    "react-router-dom": "^5.2.0",
    "react-scripts": "4.0.1",
    "wallpaper": "^4.4.2",
    "web-vitals": "^1.1.0"
  },
  "scripts": {
    "start": "cross-env NODE_ENV=development concurrently \"yarn run client\" \"wait-on http://localhost:3000 && yarn run electron:watch\" ",
    "build": "yarn run build-client && yarn run build-electron",
    "client": "set BROWSER=none&&react-app-rewired start",
    "electron:watch": "nodemon --watch public/electron.js --exec electron .",
    "electron": "electron .",
    "robots": "node public/Database/index",
    "build-client": "react-app-rewired build",
    "build-electron": "electron-builder build -w",
    "test": "react-app-rewired test",
    "eject": "react-app-rewired eject"
  },
  "build": {
    "productName": "electron-demos",
    "files": [
      "build/",
      "main.js"
    ],
    "dmg": {
      "contents": [
        {
          "x": 110,
          "y": 150
        },
        {
          "x": 240,
          "y": 150,
          "type": "link",
          "path": "/Applications"
        }
      ]
    },
    "win": {
      "target": [
        {
          "target": "nsis",
          "arch": [
            "x64"
          ]
        }
      ]
    },
    "directories": {
      "buildResources": "assets",
      "output": "release"
    }
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
